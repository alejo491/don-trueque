	<table>
	<tr>
				<td>
				<com:TLabel ID="lblfecdesd" Text="Desde: "/>      
				</td>
				<td>                        
				<com:TDatePicker ID="datedesd"
					DateFormat="yyyy-MMMM-dd"
					Mode="ImageButton"
					  FromYear="1965"
					   UpToYear="2013"
					Culture="es"
					InputMode="DropDownList"/>												   
				</td>							
		</tr>	
			
				<tr>
					<td>
					<com:TLabel ID="lblfechast" Text="Hasta: "/>
					</td>
					<td>
					<com:TDatePicker ID="datehast"
						DateFormat="yyyy-MMMM-dd"
